{
  "version": "1.7.5",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "build"
  },
  "filesGlob": [
    "app/**/*.ts",
    "typings/main/**/*.d.ts",
    "!node_modules/**"
  ],
  "exclude": [
    "node_modules",
    "client"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "app/client/client.ts",
    "app/config.ts",
    "app/custom_typings/ar_drone/index.d.ts",
    "app/index.ts",
    "app/server/constants.ts",
    "app/server/control.ts",
    "app/server/drone.ts",
    "app/server/event_publisher.ts",
    "app/server/mission_capture.ts",
    "app/server/missions_executor.ts",
    "app/server/models/commands.ts",
    "app/server/models/drone_state.ts",
    "app/server/models/operator_state.ts",
    "app/server/operator.ts",
    "app/server/PaVEParser.ts",
    "app/server/server.ts",
    "typings/main/ambient/es6-shim/index.d.ts",
    "typings/main/ambient/express-serve-static-core/index.d.ts",
    "typings/main/ambient/express/index.d.ts",
    "typings/main/ambient/form-data/index.d.ts",
    "typings/main/ambient/jasmine/index.d.ts",
    "typings/main/ambient/jquery/index.d.ts",
    "typings/main/ambient/mime/index.d.ts",
    "typings/main/ambient/node/index.d.ts",
    "typings/main/ambient/serve-static/index.d.ts",
    "typings/main/definitions/request/index.d.ts"
  ]
}
